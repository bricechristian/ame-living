<style>
a:hover {
	color: #323132 !important;
	text-decoration: none !important;
}
.main-header {
	background-color: #f8f4f1;
	max-height: 80px;
}
.announcement-bar {
	background: #e7d8ca;
	z-index: 1000000;
	text-align: center;
	position: relative;
	padding: 20px;
	color: #323132;
}
.main-nav {
	padding-left: 0;
	margin-bottom: 0;
}
.primary-nav-wrapper {
	z-index: 1000000;
	position: relative;
	background-color: #f8f4f1;
	transition: background-color 0.3s ease;
}
@media (max-width: 1200px) {
	.primary-nav-wrapper {
		padding-top: 16px;
		padding-bottom: 16px;
		padding-left: 15px;
		padding-right: 15px;
	}
}
.main-nav li, .main-header li, .shop-nav li {
	list-style: none;
	margin-left: 18px;
	margin-right: 18px;
}
@media (max-width: 1400px) {
	.main-nav li, .main-header li, .shop-nav li {
		margin-left: 12px;
		margin-right: 12px;
	}
}
.main-nav a, .shop-nav a {
	padding-top: 35px;
	padding-bottom: 35px;
	color: #323132;
	text-transform: uppercase;
	display: inline-block;
	position: relative;
	white-space: nowrap;
	font-family: AkkuratMono, sans-serif;
	font-size: 9px;
	letter-spacing: 1.8px;
	line-height: 10px;
}
.main-nav a, .shop-nav a {
	padding-top: 35px;
	padding-bottom: 35px;
	color: #323132;
	text-transform: uppercase;
	display: inline-block;
	position: relative;
	white-space: nowrap;
	text-decoration: none !important;
}
.main-nav a:hover, .shop-nav a:hover {
	text-decoration: none;
}
.main-nav a::before, .shop-nav a::before {
	content: "";
	position: absolute;
	top: 3px;
	left: 0;
	right: 0;
	height: 4px;
	opacity: 0;
	transition: all 0.4s ease;
	background: #c8a895;
}
.main-nav a:hover::before, .shop-nav a:hover::before, .main-nav li.current-menu-item a::before, .shop-nav li a.active::before {
	opacity: 1;
	top: 0;
}
.menu-primary-navigation-container {
	width: 100%;
}
.menu-secondary-navigation-container {
	width: 90%;
}
.mega-sub-nav {
	width: 30%;
	min-width: 250px;
}
.search-nav-item, .account-nav-item {
	min-width: 18px;
	margin-left: 3vw !important;
}
header .nav--desktop {
	display: flex !important;
}
@media (max-width: 1200px) {
	header .nav--desktop {
		display: none !important;
	}
}
.font-cadiz-light {
    font-family: Cadiz-Light,serif!important;
}
header .nav--mobile {
	display: none !important;
}
@media (max-width: 1200px) {
	header .nav--mobile {
		display: flex !important;
	}
}
header .header__icons {
    position: relative;
    top: 2px;
}
header .header__icon--cart {
    left: 0;
    margin: 0 18px;
    width: 20px;
    height: auto;
}
header .header__icon--search {
  width: 18px;
  margin: 0 18px 0 3vw;
}
header .header__icon .icon-search {
  width: 20px;
  height: 20px;
}
header .is-dropdown a {
  color: #323132;
}
header .wps-items.wps-items-list {
    display: grid;
    grid-template-columns: repeat(4, 1fr);
    gap: 40px 20px;
    padding: 0px;
    transition: opacity 0.3s ease 0s;
    opacity: 1;
}
.mobile-nav-sec {
	margin-top: 40px;
	height: 47vh;
	overflow-y: scroll;
	padding-bottom: 50px;
	padding-right: 10px;
}
.mobile-nav-sec .contact-wrap {
	width: 100%;
	text-align: center;
}
.mobile-nav-sec .open-close {
	display: inline-block;
	right: 0;
	color: #323132;
	border-radius: 3px;
	position: absolute;
	line-height: 0;
	font-size: 13px;
	vertical-align: middle;
	padding: 0;
	margin-top: 0;
	transition: all 0.4s ease;
	width: 28px;
	text-align: center;
}
.mobile-nav-sec .open-close .icon::before {
	transition: all 0.4s ease;
}
@media (max-width: 1200px){
  .sticky-wrapper {
      position: relative;
      z-index: 999999;
  }
}
.mobile-menu {
	position: absolute;
	left: 0;
	right: 0;
	bottom: 0;
	z-index: 100000;
	top: 0;
	padding-top: 85px;
	display: none;
	transition: all 0.2s ease;
	padding-left: 30px;
	padding-right: 30px;
	max-height: 100vh;
}
.mobile-menu.go {
	display: block;
}
.mobile-menu ul li {
	text-align: left;
	margin-left: 0;
	margin-right: 0;
}
.mobile-menu ul li a::after {
	display: none;
}
.mobile-menu-links {
	transform: translateY(-50px);
	transition: all 0.3s ease;
}
.mobile-menu .footer-sm {
	position: absolute;
	bottom: 0;
	left: 0;
	right: 0;
}
.mobile-menu .footer {
	padding-bottom: 20px;
}
.menu-mobile-navigation-container, .menu-mobile-navigation-container ul, .menu-mobile-navigation-container ul li {
	width: 100%;
}
.menu-mobile-navigation-container ul.sub-menu {
	border-top: 1px solid #303753;
	border-bottom: 1px solid #303753;
	margin-top: 17px;
	padding-top: 17px;
	padding-bottom: 17px;
	margin-bottom: 17px;
	display: none;
	max-height: 0;
	height: 0;
	transition: max-height 0.3s ease;
	overflow: hidden;
}
.nav--mobile .search--icon {
    width: 22px;
    height: 22px;
    position: relative;
    bottom: 1px;
}
.mobile-footer-nav {
  padding: 13px 0;
}
@media (max-width: 992px){
  .footer .social-icons {
    padding-top: 21px !important;
    padding-bottom: 21px !important;
  }
}
/** hamburger */

.hamburger-inner {
	margin-top: 0;
}
.hamburger-inner, .hamburger-inner::after, .hamburger-inner::before {
	width: 26px !important;
	height: 1px !important;
	background-color: #b3a091 !important;
}
.hamburger-inner::before {
	top: -7px !important;
}
.hamburger-inner::after {
	bottom: -7px !important;
}
.hamburger-box {
	width: 25px !important;
	height: 20px !important;
}
.hamburger--slider .hamburger-inner {
	top: 10px !important;
}
.hamburger--slider .hamburger-inner::after {
	top: 7px !important;
}
.hamburger {
	padding: 0 0 0 !important;
	height: 20px !important;
	display: flex;
}
.hamburger--slider.is-active .hamburger-inner::after {
	top: 20px !important;
	background-color: #323132 !important;
}
.hamburger--slider.is-active .hamburger-inner {
	top: 2px !important;
	background-color: #323132 !important;
}
.stuck {
	position: fixed;
	top: 0;
	left: 0;
	right: 0;
	z-index: 100000;
}
a.brand img {
	max-width: 110px;
}
@media (max-width: 768px) {
	a.brand img {
		max-width: 90px;
	}
}
.person--icon {
	color: #646970;
}  
</style>

